{
    "providers": [
        "awscloudformation"
    ],
    "projectName": "amplify-react-app",
    "version": "3.0",
    "frontend": "javascript",
    "javascript": {
        "framework": "react",
        "config": {
            "SourceDir": "https://github.com/gabrielcerutti/amplify-react-app.git",
            "DistributionDir": "build",
            "BuildCommand": "npm.cmd run-script build",
            "StartCommand": "npm.cmd run-script start"
        }
    }
}